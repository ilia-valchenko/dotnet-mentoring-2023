@using Client.Web.Models
@model List<Category>

<h1>Client Secret Page</h1>

<h2>List of categories:</h2>

<ul>
    @foreach (var category in Model)
    {
        <br />
        <br />
        <br />
        <li>@category.Id</li>
        <li>@category.Name</li>
        <li>@category.ParentCategoryId</li>
        <li>@category.ImageUrlText</li>
    }
</ul>