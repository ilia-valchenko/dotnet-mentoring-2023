{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MessageBrokerConfiguration": {
    "Uri": "amqp://guest:guest@localhost:5672",
    "ClientProviderName": "Rabbit Receiver App",
    "ExchangeName": "DemoExchange",
    "RoutingKey": "demo-routing-key",
    "QueueName": "DemoQueue"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
